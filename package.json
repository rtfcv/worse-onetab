    "daisyui": "^2.14.3",
    "react": "^18.1.0",
    "react-daisyui": "^2.0.1",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@tailwindcss/typography": "^0.5.2",
    "@types/chrome": "^0.0.184",
    "@types/react": "^18.0.8",
